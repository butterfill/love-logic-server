head
  title love-logic
  meta( name="viewport", content="width=device-width, initial-scale=1.0" )
  link(href="https://fonts.googleapis.com/icon?family=Material+Icons",rel="stylesheet")
  script(type="text/javascript" src="https://www.google.com/jsapi")
body


template(name='ApplicationLayout')
  +header
  .container
    +Template.dynamic(template=main)

template(name='header')
  nav.grey(role='navigation')
    .nav-wrapper.container
      a.brand-logo.huge-glow-40(href='/') logic-ex
      .right
        span #{currentUserEmail}
        +atNavButton
      if isBrowserNotCompatible
        .container
          p.red.center Your web browser is too old for this site: there may be errors.
            a(href='/oldBrowserSorry') more info
